{
  "name": "simple-ts-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn install && ts-node-dev --ignore-watch ./node_modules --transpile-only --respawn -r tsconfig-paths/register ./src/server.ts",
    "routes": "yarn dev --show-routes",
    "build": "tsc -p .",
    "start": "yarn build && yarn install && ts-node-dev --ignore-watch ./node_modules --transpile-only --respawn -r dotenv/config ./src/server.ts",
    "lint": "eslint --fix ./src/**/*.ts",
    "format": "prettier --write . --loglevel silent",
    "test": "jest",
    "run:llama": "ts-node-dev --quiet ./src/environment/scripts/llama.ts"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn build && yarn lint && yarn test",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.15.11",
    "@types/node-json-db": "^0.9.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "axios": "^0.25.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "express-async-errors": "^3.1.1",
    "husky": "^4.3.0",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.4.3",
    "jest": "^28.1.3",
    "morgan": "^1.10.0",
    "msw": "^0.44.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "apicache-plus": "^2.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mongoose-query-parser": "^1.2.1",
    "node-cron": "^3.0.0",
    "node-json-db": "^1.4.1",
    "prettyjson": "^1.2.5",
    "sift": "^15.1.3",
    "ts-jest": "^28.0.7",
    "tsconfig-paths": "^3.12.0"
  }
}
